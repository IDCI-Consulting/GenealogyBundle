{
    "id": "{{ element.id }}",
    "name": "{{ element.name }}",
    "birthdate": "{{ element.birthdate|date('d/m/Y')}}",
    "sex": {% if(element.sex==0) %}"male"{% else %}"femelle"{% endif %},
    {% include 'IDCIGenealogyBundle:JSON:functions.json.twig' with {'roles': element.roles} %}
    {% if element.getChildren|length > 0 %}
        {% if element.getChildren|length > 1 %}
            "children":
            [
            {% for element in element.getChildren %}
                {% include 'IDCIGenealogyBundle:JSON:element.json.twig' with {'element': element} %}
                {% if not loop.last %}
                ,
                {% endif %}
            {% endfor %}
            ],
        {% else %}
            "child":
            {% include 'IDCIGenealogyBundle:JSON:element.json.twig' with {'element': element.getChildren.first } %},
        {% endif %}
    {% endif %}

    {# {% if (element.hasMedia()) %}
        {% include 'IDCIGenealogyBundle:JSON:medias.json.twig' with {'medias': element.medias} %}
    {% endif %} #}

    "bornPlace":{}
}