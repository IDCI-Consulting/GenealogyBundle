{% extends "IDCIGenealogyBundle::adminbase.html.twig" %}

{% block nav %}
  {% include "IDCIGenealogyBundle::entityNav.html.twig" with {'entities' : {
    'Elements':      { 'basepath': 'element'},
    'Images':        { 'basepath': 'image'},
    'Races':         { 'basepath': 'race'},
    'Roles':         { 'basepath': 'role'},
    'Videos':        { 'basepath': 'video', 'active': true },
  }} %}
{% endblock %}

{% block body %}

    {{ parent() }}

    <div class="page-header">
        <h1>{% trans %}Youtube Video edit{% endtrans %}</h1>
    </div>

    {{ form(edit_form) }}

    <ul class="record_actions">
        <li>
            <a href="{{ path('video') }}">
                {% trans %}Back to the list{% endtrans %}
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>

{% endblock body %}