<?xml version="1.0" encoding="UTF-8"?>
<element id="{{ entity.id }}">
    {% include 'IDCIGenealogyBundle:XML:element.xml.twig' with {'element': entity} %}
    <children>
    {% for child in entity.children %}
         <child id="{{ child.id }}">
            {% include 'IDCIGenealogyBundle:xml:element..twig' with {'element': child} %}
         </child>
    {% endfor %}
    </children>
</element>